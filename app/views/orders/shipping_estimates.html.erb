<table class="table">

  <% @response.each do |key, value| %>
    <% value.each do |delivery_type| %>
      <tr>
        <td><%= delivery_type[0] %></td>
        <td><%= delivery_type[1] %></td>
        <td><%= link_to "Select", order_select_shipping_method_path(delivery_method: delivery_type[0], shipping_cost: delivery_type[1]) %></td>
      </tr>
    <% end %>
  <% end %>

</table>

<div class="row">
  <div class="col-md-offset-11">
    <%= link_to "Payment Details", edit_order_path(current_order.id), class: "btn btn-primary" %>
  </div>
</div>
